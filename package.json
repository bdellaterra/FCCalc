{
  "name": "FCCalc",
  "version": "0.1.0",
  "description": "FCCalc react app",
  "main": "client.js",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "redux-act": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "nyc": "^10.1.2",
    "style-loader": "^0.13.1",
    "teaspoon": "^6.4.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "os": "linux",
  "ava": {
    "files": [
      "test/*test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack -d && webpack-dev-server --watch-poll",
    "build": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "cover": "cross-env JS_TEST_LIB=ava nyc ava",
    "test": "cross-env JS_TEST_LIB=ava ava --tap",
    "watch": "cross-env JS_TEST_LIB=ava ava --watch --verbose"
  },
  "author": "Brian Dellaterra",
  "license": "LGPL-3.0"
}
